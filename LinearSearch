#include <stdio.h>
int main()
{
    int a[50];
    int n,f,ch;
    ch=0;
    printf("Input number of elements:");
    scanf("%d",&n);
    printf("Enter elements :- ");
    for(int i = 0;i<n;i++)
    {
        scanf("%d",&a[i]);
        
    }
    printf("Elements are --> \n");
    for(int i = 0;i<n;i++)
    {
        printf("%d ",a[i]);
        
    }
    printf("\n Elements to be searched --> \n");
    scanf("%d",&f);
    for(int i = 0;i<n;i++)
    {
        ch++;
        if(a[i]==f)
        {
            printf("Element found at %d th position", ch);
            break;
        }
        
    }
    
    
    return 0;
}

